{
  "name": "timer-server",
  "version": "1.0.0",
  "description": "Uses Apollo/GraphQL, accepts requests to add terminations to a zoom session and updates them into CockroachDB",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --detectOpenHandles --config=jest.config.js",
    "start": "export $(cat .env | xargs) && nodemon src/index.js",
    "start:ci": "export $(cat .env | xargs) && node src/index.js"
  },
  "keywords": [
    "timer-server",
    "apollo-server",
    "cockroachDB"
  ],
  "author": "Nir Alfasi",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.17.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "moment": "^2.27.0",
    "nodemon": "^1.19.4",
    "sequelize": "^6.3.5",
    "sequelize-cockroachdb": "^1.1.0"
  },
  "devDependencies": {
    "apollo": "^2.1.8",
    "jest": "^25.0.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__utils"
    ]
  }
}
